  {{SERVICE_NAME}}:
    image: neo4j:${NEO4J_VERSION:-community}
    container_name: ${PROJECT_NAME}_{{SERVICE_NAME}}
    restart: unless-stopped
    ports:
      - "{{SERVICE_PORT}}:7474"
      - "7687:7687"
    environment:
      NEO4J_AUTH: neo4j/${NEO4J_PASSWORD:-password}
    volumes:
      - ./services/{{SERVICE_NAME}}/data:/data
    networks:
      - nself_network
